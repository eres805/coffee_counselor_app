<h1>Roasters</h1>

<table>
	<thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

 <tbody>
 		<% @roasters.each do |roaster| %>
      <tr>
        <td><%= link_to roaster.roaster_name, roaster -%></td>
        <td><%= roaster.address %></td>
        <td><%= roaster.website_url %></td>
      </tr>
    <% end %>
  </tbody>
</table>

